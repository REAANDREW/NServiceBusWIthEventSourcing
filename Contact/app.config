<?xml version="1.0" encoding="utf-8"?>

<configuration>
  <configSections>
    <section name="UnicastBusConfig" type="NServiceBus.Config.UnicastBusConfig, NServiceBus.Core" />
    <section name="MessageForwardingInCaseOfFaultConfig"
             type="NServiceBus.Config.MessageForwardingInCaseOfFaultConfig, NServiceBus.Core" />
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net" />
    <section name="Logging" type="NServiceBus.Config.Logging, NServiceBus.Core" />
    <section name="MsmqTransportConfig" type="NServiceBus.Config.MsmqTransportConfig, NServiceBus.Core" />
  </configSections>
  <connectionStrings>
    <add name="TestDatabase" connectionString="Server=localhost;Database=EventStore;Integrated Security=True" providerName="System.Data.SqlClient" />
  </connectionStrings>
  <Logging Threshold="ERROR" />
  <MsmqTransportConfig
    NumberOfWorkerThreads="20"
    MaxRetries="5" />
  <UnicastBusConfig>
    <MessageEndpointMappings>
      <add Messages="Contact.Messages.Commands.ApproveAccLead, Contact.Messages" Endpoint="Contact.Commands.ApproveAccLead" />
      <add Messages="Contact.Messages.Commands.CreateAccommodationLead, Contact.Messages" Endpoint="Contact.Commands.CreateAccommodationLead" />
      <add Messages="Contact.Messages.Commands.CreateAccSupplier, Contact.Messages" Endpoint="Contact.Commands.CreateAccSupplier" />
      <add Messages="Contact.Messages.Commands.CreateAuthenticationWithGeneratedPassword, Contact.Messages" Endpoint="Contact.Commands.CreateAuthenticationWithGeneratedPassword" />
      <add Messages="Contact.Messages.Commands.CreateUser, Contact.Messages" Endpoint="Contact.Commands.CreateUser" />
      <add Messages="Contact.Messages.Events.AccommodationLeadApproved, Contact.Messages" Endpoint="Contact.Events.AccommodationLeadApproved" />
      <add Messages="Contact.Messages.Events.AccommodationLeadCreated, Contact.Messages" Endpoint="Contact.Events.AccommodationLeadCreated" />
      <add Messages="Contact.Messages.Events.AccommodationSupplierCreated, Contact.Messages" Endpoint="Contact.Events.AccommodationSupplierCreated" />
      <add Messages="Contact.Messages.Events.AuthenticationCreated, Contact.Messages" Endpoint="Contact.Events.AuthenticationCreated" />
      <add Messages="Contact.Messages.Events.UserCreated, Contact.Messages" Endpoint="Contact.Events.UserCreated" />
    </MessageEndpointMappings>
  </UnicastBusConfig>
  <MessageForwardingInCaseOfFaultConfig ErrorQueue="error" />
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="NServiceBus" publicKeyToken="9fc386479f8a226c" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-3.3.0.0" newVersion="3.3.0.0" />
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="StructureMap" publicKeyToken="e60ad81abae3c223" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-2.6.4.0" newVersion="2.6.4.0" />
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="NServiceBus.Core" publicKeyToken="9fc386479f8a226c" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-3.3.0.0" newVersion="3.3.0.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
  <log4net debug="false">
    <appender name="RollingFile" type="log4net.Appender.RollingFileAppender">
      <file value="output.log" />
      <appendToFile value="true" />
      <rollingStyle value="Once" />
      <maximumFileSize value="100KB" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%level %thread %logger - %message%newline" />
      </layout>
    </appender>
    <root>
      <level value="ALL" />
      <appender-ref ref="RollingFile" />
    </root>
  </log4net>
</configuration>